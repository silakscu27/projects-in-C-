#include <stdio.h>

 int main()
 {
 int s1,s2; 
 
 printf( "Input two integers: " ); 
 scanf( "%d%d", &s1, &s2 );

 if ( s1 % s2 == 0 ) {
 printf( "%d is a multiple of %d ", s1, s2 );
 printf( "by a factor of %d\n", s1 / s2 );
 } 

 if ( s1 % s2 != 0 ) {
 printf( "%d is not a multiple of %d\n", s1, s2 );
 } 
 
 return 0; 
}
