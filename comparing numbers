#include <stdio.h>

 int main()
 {
 int x; 
 int y; 

 printf( "Enter two numbers: " ); 
 scanf( "%d%d", &x, &y ); 

 /* compare the two numbers */
 if ( x > y ) {
 printf( "%d is larger\n", x );
 } 

 if ( x < y ) {
 printf( "%d is larger\n", y );
 } 

 if ( x == y ) {
 printf( "These numbers are equal\n" );
 }
return 0;
}
